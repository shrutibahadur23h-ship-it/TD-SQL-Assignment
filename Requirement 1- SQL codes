    `column_16` INT, `column_17` INT, `column_18` INT, `column_19` INT, `column_20` INT
);

INSERT INTO `Feed-3`
SELECT
    FLOOR(RAND() * 1000), FLOOR(RAND() * 1000), FLOOR(RAND() * 1000), FLOOR(RAND() * 1000), FLOOR(RAND() * 1000),
    FLOOR(RAND() * 1000), FLOOR(RAND() * 1000), FLOOR(RAND() * 1000), FLOOR(RAND() * 1000), FLOOR(RAND() * 1000),
